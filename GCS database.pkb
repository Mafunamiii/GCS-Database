-------VIEW PROJECT 1 (as shown in the example given in the PDF)
CREATE VIEW ViewProj_2 AS 
SELECT EST_START_DATE, EST_END_DATE, PROJECT_TASK, SKILL, EMPLOYEE, ACTUAL_START_DATE, ACTUAL_END_DATE
FROM 
    (SELECT PROJECT_TASK, EST_START_DATE, EST_END_DATE,  
        LISTAGG(SKILL,            chr(10)) WITHIN GROUP (ORDER BY SKILL)            SKILL, 
        LISTAGG(EMPLOYEE,         chr(10)) WITHIN GROUP (ORDER BY EMPLOYEE)         EMPLOYEE, 
        LISTAGG(ACTUAL_START_DATE,chr(10)) WITHIN GROUP (ORDER BY ACTUAL_START_DATE)ACTUAL_START_DATE,
        LISTAGG(ACTUAL_END_DATE,  chr(10)) WITHIN GROUP (ORDER BY ACTUAL_END_DATE)  ACTUAL_END_DATE
    FROM
        (SELECT T.TASK_DESC AS PROJECT_TASK , 
            T.TASK_START    AS EST_START_DATE, 
            T.TASK_END      AS EST_END_DATE, 
            S.SKILL_DESC    AS SKILL, 
            T.TASK_ID , 
            T.PROJ_ID || A.ASSIGN_ID || '-' || E.EMP_LNAME || ' ' || SUBSTR(E.EMP_FNAME, 1, 1) || '.' AS EMPLOYEE,
            A.ASSIGN_START  AS ACTUAL_START_DATE, 
            A.ASSIGN_END    AS ACTUAL_END_DATE
        FROM ((((PLAN P 
            INNER JOIN SKILL S      ON P.SKILL_ID = S.SKILL_ID) 
            INNER JOIN TASK T       ON P.TASK_ID  = T.TASK_ID)
            INNER JOIN ASSIGNMENT A ON P.PLAN_ID  = A.PLAN_ID)
            INNER JOIN EMPLOYEE E   ON A.EMP_ID   = E.EMP_ID)
        WHERE T.PROJ_ID = 1
        ORDER BY P.TASK_ID)
    GROUP BY PROJECT_TASK, EST_START_DATE, EST_END_DATE, TASK_ID
    ORDER BY TASK_ID)
    
